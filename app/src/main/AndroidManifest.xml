<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.katiabravo.tour_sanfrancisco">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Beaches"
            android:label="Beaches" />
        <activity android:name=".FoodRelated"
            android:label="Food Related" />
        <activity android:name=".Museums"
            android:label="Museums" />
        <activity android:name=".HistoricalSites"
            android:label="Historical Sites" />
    </application>

</manifest>